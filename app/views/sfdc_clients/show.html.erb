<div class="main-wrapper">
  <img src="<%= h(gravatar_url(@client.email__c, :size => 80)) %>" alt="Gravatar"/>
  <p>Name: <%= @name %></p>
  <p>Address: <%= @address %></p>
  <p>Bio: <%= @bio %></p>
  <p>Inquiries: <%= @inquiries %></p>
  <p>Price: <%= @price %></p>
  <p>Rating: <%= @rating %></p>
  <p>Email: <%= @email %></p>
  <p>Phone: <%= @phone %></p>
  <p>Industry: <%= @industry %></p>
  <p>Primary Contact: <%= @primary_contact %></p>
</div>